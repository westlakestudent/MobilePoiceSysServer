<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
           "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="com.zjedu.po.Help" table="help">
        <id name="id" column="id" type="int">
            <generator class="increment"/>
        </id>
        <property name="addr" column="addr" type="string" not-null="false"/>
        <property name="content" column="content" type="string" not-null="false"/>
        <property name="username" column="username" type="string" not-null="false"/>
        <property name="time" column="time" type="string" not-null="false"/>
    </class>
</hibernate-mapping>